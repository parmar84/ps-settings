<keymap version="1" name="Emacs (GMap)" parent="Emacs">
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="shift ctrl minus" />
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="ActivateTODOToolWindow">
    <keyboard-shortcut first-keystroke="alt equals" />
  </action>
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="shift ctrl alt windows" second-keystroke="g" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="0" />
  </action>
  <action id="CloseEditor">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="k" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="shift ctrl subtract" />
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="EditorAddOrRemoveCaret">
    <mouse-shortcut keystroke="alt button1" />
    <mouse-shortcut keystroke="control alt button1" />
  </action>
  <action id="EditorDuplicate">
    <keyboard-shortcut first-keystroke="ctrl d" />
  </action>
  <action id="EditorPageUp">
    <keyboard-shortcut first-keystroke="page_up" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="EditorToggleShowLineNumbers">
    <keyboard-shortcut first-keystroke="shift ctrl alt windows" second-keystroke="n" />
  </action>
  <action id="ExpandRegionRecursively">
    <keyboard-shortcut first-keystroke="ctrl alt add" />
  </action>
  <action id="GitToolBox.BlameDetails">
    <keyboard-shortcut first-keystroke="shift ctrl alt windows" second-keystroke="b" />
  </action>
  <action id="GotoTest" />
  <action id="HideAllWindows">
    <keyboard-shortcut first-keystroke="shift ctrl f12" />
    <keyboard-shortcut first-keystroke="ctrl alt equals" />
  </action>
  <action id="Images.ShowThumbnails" />
  <action id="MoveEditorToOppositeTabGroup">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="t" />
  </action>
  <action id="OpenEditorInOppositeTabGroup">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="d" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="shift ctrl alt tab" />
  </action>
  <action id="RunDashboard.ShowConfigurations" />
  <action id="SurroundWith">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt windows" second-keystroke="d" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="shift ctrl alt windows" second-keystroke="z" />
  </action>
</keymap>